{
  "name": "@esim-go/workers",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "dev:workers": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "test": "bun src/index.ts",
    "sync": "bun src/index.ts --sync",
    "sync:manual": "bun src/index.ts sync",
    "sync:clean": "bun src/index.ts --clean",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "db:types": "./scripts/sync-db-types.sh",
    "db:types:bun": "bun scripts/generate-db-types.ts",
    "postinstall": "bun run db:types"
  },
  "dependencies": {
    "@esim-go/client": "workspace:*",
    "@esim-go/utils": "workspace:*",
    "@supabase/supabase-js": "^2.38.0",
    "bullmq": "^5.0.0",
    "byte-size": "^9.0.1",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "i18n-iso-countries": "^7.14.0",
    "ioredis": "^5.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/byte-size": "^8.1.2",
    "@types/node": "^20.10.0",
    "eslint": "^8.53.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
}