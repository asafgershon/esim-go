# Environment
NODE_ENV=development
LOG_LEVEL=info

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_MAX_RETRIES=3

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# eSIM Go API Configuration
ESIM_GO_API_KEY=your-api-key
ESIM_GO_BASE_URL=https://api.esim-go.com/v2.5
ESIM_GO_RETRY_ATTEMPTS=3

# Worker Configuration
WORKER_CONCURRENCY=5
SYNC_INTERVAL=0 0 * * *
STUCK_JOB_THRESHOLD=60
CLEANUP_OLD_JOBS_DAYS=30

# Queue Configuration
JOB_ATTEMPTS=3
JOB_BACKOFF_DELAY=2000
JOB_REMOVE_ON_COMPLETE_AGE=3600
JOB_REMOVE_ON_COMPLETE_COUNT=1000
JOB_REMOVE_ON_FAIL_AGE=86400