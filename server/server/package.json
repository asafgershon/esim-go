{
  "name": "esim-go-server",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "codegen:dbtypes": "bunx supabase gen types typescript --project-id \"dgkyjkzkwzmjjurzvcxy\" --schema public > src/database.types.ts || echo \"Skipping Supabase codegen (no token available)\"",
    "build": "bun run codegen",
    "dev": "bun run --watch src/app.ts",
    "dev:server": "bun run --watch src/app.ts",
    "debug": "bun --inspect --watch src/app.ts",
    "start": "bun run build && bun run src/app.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "postinstall": "bun run codegen && bun run codegen:dbtypes"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@parcel/watcher": "^2.5.1",
    "@types/bun": "latest",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/qrcode": "^1.5.5",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.0",
    "@types/byte-size": "^8.1.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@apollo/datasource-rest": "^6.4.1",
    "@apollo/server": "^4.12.2",
    "@apollo/utils.keyvadapter": "^4.0.1",
    "byte-size": "^9.0.1",
    "@esim-go/client": "workspace:*",
    "@esim-go/rules-engine": "workspace:*",
    "@esim-go/utils": "workspace:*",
    "@graphql-tools/merge": "^9.1.1",
    "@keyv/redis": "^4.6.0",
    "@supabase/supabase-js": "^2.47.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.18.1",
    "bullmq": "^5.56.5",
    "cors": "^2.8.5",
    "countries-list": "^3.1.1",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "filesize": "^11.0.2",
    "graphql": "^16.11.0",
    "graphql-redis-subscriptions": "^2.7.0",
    "graphql-ws": "^6.0.5",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "keyv": "^5.3.4",
    "nanoid": "^5.1.5",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "qrcode": "^1.5.4",
    "redis": "^5.6.0",
    "ws": "^8.18.2",
    "zod": "^3.25.67"
  },
  "trustedDependencies": [
    "@parcel/watcher"
  ]
}
