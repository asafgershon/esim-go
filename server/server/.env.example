# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# eSIM Go API Configuration
ESIM_GO_API_KEY=your-esim-go-api-key
ESIM_GO_BASE_URL=https://api.esim-go.com/v2.5
ESIM_GO_WEBHOOK_SECRET=your-webhook-secret

# Server Configuration
PORT=4000
NODE_ENV=development

# Database Configuration (for direct database access if needed)
DATABASE_URL=postgresql://postgres:password@localhost:5432/esim_go

# Redis Configuration (optional, for caching and subscriptions)
REDIS_URL=redis://localhost:6379

# JWT Configuration (legacy support)
JWT_SECRET=your-jwt-secret

# Logging
LOG_LEVEL=info
