{
  "name": "@hiilo/rules-engine-2",
  "version": "1.0.0",
  "description": "Pure pricing rules engine for eSIM Go",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bun tsc -p tsconfig.build.json",
    "dev": "bun tsc --watch",
    "codegen:dbtypes": "bunx supabase gen types typescript --project-id \"dgkyjkzkwzmjjurzvcxy\" --schema public > src/generated/database.types.ts || echo \"Skipping Supabase codegen (no token available)\"",
    "clean": "rm -rf dist",
    "codegen": "graphql-codegen",
    "generate": "graphql-codegen",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hiilo/utils": "workspace:*",
    "@supabase/supabase-js": "^2.53.0",
    "dot-object": "^2.1.5",
    "envalid": "^8.1.0",
    "immer": "^10.1.1",
    "json-rules-engine": "^7.3.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@types/dot-object": "^2.1.6",
    "@types/node": "^20.0.0",
    "@vitest/ui": "^3.2.4",
    "csv": "^6.4.1",
    "typescript": "^5.2.0",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "esim-go",
    "pricing",
    "rules-engine",
    "typescript"
  ]
}